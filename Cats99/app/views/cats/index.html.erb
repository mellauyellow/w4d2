<h1>C A T S</h1>

<ul>
  <% @cats.each do |cat| %>
    <li>Name: <%= cat.name %></li>
    <li>Age: <%= cat.age %></li>
    <li>Color: <%= cat.color %></li>
    <%= link_to "Show more info...", "/cats/#{cat.id}" %>
    <%#= button_to "Show more info...", cat_url(cat), method: :get %>
    <br>
    <br>
  <% end %>
</ul>
